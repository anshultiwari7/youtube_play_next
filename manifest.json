{
    "name": "test",
    "description" : "testing extension",
    "version": "1.0",
    "author":"Anshul Tiwari",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "toggle_switch.html",
      "default_icon": "queue.png"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens queue.html"
      }
    },
    "permissions": ["https://www.youtube.com/", "storage"],
    "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "js": ["content.js"],
        "run_at": "document_end"         // pay attention to this line
        }
    ],
    "background": {
    "scripts": ["background.js", "backgroundContextMenus.js"],
    "persistent": false
  }
}
